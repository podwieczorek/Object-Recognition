{
  "expo": {
    "name": "Object Recognition",
    "slug": "Object-Recognition",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/object_recognition.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/object_recognition2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Enable camera access",
        "NSMicrophoneUsageDescription": "Enable microphone access"
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/object_recognition.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "package": "com.podwieczorek.ObjectRecognition"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c7412354-30ac-40c4-8980-4a2aba9a9165"
      }
    }
  }
}
